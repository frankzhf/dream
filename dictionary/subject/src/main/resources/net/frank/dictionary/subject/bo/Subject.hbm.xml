<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="net.frank.dictionary.subject.bo.Subject" table="TB_DICTIONARY">
	    <cache usage="read-write" region="dictionaryCache" />
		<id name="id" type="java.lang.Long">
			<column name="ID" />
			<generator class="native" />
		</id>
		<property name="codeId" type="java.lang.String">
			<column name="CODE_ID" />
		</property>
		<property name="codeDesc" type="java.lang.String">
			<column name="CODE_DESC" />
		</property>
		<many-to-one name="children$3" class="net.frank.framework.bo.Resource" lazy="false">
			<column name="CHILDREN$3" />
		</many-to-one>
		<many-to-one name="resource" class="net.frank.framework.bo.Resource" lazy="false">
			<column name="RESOURCE_" />
		</many-to-one>
	</class>
</hibernate-mapping>
