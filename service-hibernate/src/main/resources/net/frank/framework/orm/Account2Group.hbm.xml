<?xml version="1.0"?>
<hibernate-mapping>
	<class name="net.frank.framework.bo.Account2Group" table="TB_ACCOUNT_TO_GROUP" >
		<id name="id" type="java.lang.Long">
			<column name="ID" />
			<generator class="native" />
		</id>
		<many-to-one name="account$5" class="net.frank.framework.bo.Resource" lazy="false">
			<column name="ACCOUNT$5" />
		</many-to-one>
		<many-to-one name="group$6" class="net.frank.framework.bo.Resource" lazy="false">
			<column name="GROUP$6" />
		</many-to-one>
		<many-to-one name="resource" class="net.frank.framework.bo.Resource" lazy="false">
			<column name="RESOURCE_" />
		</many-to-one>
	</class>
</hibernate-mapping>